#include <iostream>
using namespace std;

int main()
{
    float x;
    cout << "enter x: ";
    cin >> x;

    float ch = 1;
    float zn = 1;
    float resalt;

    for (int i = 1; i <= 63; i=i * 2 + 1) {
        ch *= (x-i);
    }

    for (int i = 2; i <= 64; i *= 2) {
        zn *= (x-i);
    }

    resalt = ch / zn;
    cout << "result: " << resalt;
}
